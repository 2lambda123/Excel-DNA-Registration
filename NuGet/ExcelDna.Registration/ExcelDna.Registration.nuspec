        <dependencies>
          <group targetFramework=".NETFramework4.5.2" >
            <dependency id="ExcelDna.Integration" version="[$ExcelDnaVersion$]" />
          </group>
          <group targetFramework="net6.0-windows" >
            <dependency id="ExcelDna.Integration" version="[$ExcelDnaVersion$]" />
          </group>
        </dependencies>
    </metadata>
    <files>
        <file src="..\..\Source\ExcelDna.Registration\bin\Release\net452\ExcelDna.Registration.dll" target="lib\net452\ExcelDna.Registration.dll" />
        <file src="..\..\Source\ExcelDna.Registration\bin\Release\net452\ExcelDna.Registration.pdb" target="lib\net452\ExcelDna.Registration.pdb" />
        <file src="..\..\Source\ExcelDna.Registration\bin\Release\net6.0-windows\ExcelDna.Registration.dll" target="lib\net6.0-windows\ExcelDna.Registration.dll" />
        <file src="..\..\Source\ExcelDna.Registration\bin\Release\net6.0-windows\ExcelDna.Registration.pdb" target="lib\net6.0-windows\ExcelDna.Registration.pdb" />
    </files>
</package>
